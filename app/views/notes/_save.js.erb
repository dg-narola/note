$("ul.errors").html("")
<% if note.errors.any? %>
  <% note.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li/>").html("<%= message.html_safe %>"))
  <% end %>
<% else %>
  $(".note-index").html("<%= escape_javascript(render 'index') %>");
  $("#note-modal").modal("hide")
<% end %>
